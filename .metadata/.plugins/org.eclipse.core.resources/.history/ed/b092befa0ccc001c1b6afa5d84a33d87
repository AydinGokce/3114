import java.util.ArrayList;

public class MinHeap {
	private ArrayList<Vertex> heap;
	
	MinHeap() {
		heap = new ArrayList<Vertex>();
	}
	
	public void add(Vertex newVertex) {
		
		
		for(int i = 0; i < heap.size(); i++) {
			if(heap.get(i).getDistance() > newVertex.getDistance()) {
				heap.add(i, newVertex);
				return;
			}
		}
		heap.add(newVertex);
	}
}
