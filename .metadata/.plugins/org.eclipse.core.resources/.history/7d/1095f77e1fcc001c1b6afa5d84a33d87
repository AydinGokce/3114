
public class Edge {
	
	private int id
	private int weight;
	private ArrayList<Integer> successors; 
	
	Edge(int successor, int weight) {
		this.successor = successor;
		this.weight = weight;
	}
	
	public int getWeight() {
		return weight;
	}
	
	public int getSuccessor() {
		return successor;
	}
	
}
